{
	"name": "oyster-mp-v2-static",
	"version": "1.0.0",
	"private": true,
	"description": "",
	"templateInfo": {
		"name": "mobx",
		"typescript": true,
		"css": "scss"
	},
	"main": "index.js",
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"eslint": "eslint --ext js,jsx,ts,tsx src",
		"stylelint": "stylelint 'src/**/*.scss' --syntax scss",
		"fix": "prettier --write 'src/**/*.{tsx,jsx,js,ts,scss}'"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{tsx,jsx,js,ts}": [
			"yarn run eslint",
			"git add"
		],
		"src/**/*.{scss}": [
			"yarn run stylelint",
			"git add"
		],
		"src/**/*.{tsx,js,ts,scss}": [
			"prettier --write 'src/**/*.{tsx,jsx,js,ts,scss}'",
			"git add"
		]
	},
	"author": "benchiu<benchiu@yandex.com>",
	"license": "MIT",
	"dependencies": {
		"@tarojs/cli": "2.0.0",
		"@tarojs/components": "2.0.0",
		"@tarojs/mobx": "2.0.0",
		"@tarojs/mobx-h5": "2.0.0",
		"@tarojs/mobx-rn": "2.0.0",
		"@tarojs/rn-runner": "2.0.0",
		"@tarojs/router": "2.0.0",
		"@tarojs/taro": "2.0.0",
		"@tarojs/taro-alipay": "2.0.0",
		"@tarojs/taro-h5": "2.0.0",
		"@tarojs/taro-swan": "2.0.0",
		"@tarojs/taro-tt": "2.0.0",
		"@tarojs/taro-weapp": "2.0.0",
		"@typescript-eslint/eslint-plugin": "^2.16.0",
		"babel-runtime": "^6.26.0",
		"class": "^0.1.4",
		"classnames": "^2.2.6",
		"config": "^3.2.4",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-prettier": "^3.1.2",
		"husky": "^4.0.9",
		"lint-staged": "^9.5.0",
		"mobx": "5.15.0",
		"mockjs": "^1.1.0",
		"nerv-devtools": "^1.5.6",
		"nervjs": "^1.5.6",
		"prettier": "^1.19.1",
		"qs": "^6.9.7",
		"taro-ui": "^2.2.4",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"vconsole": "^3.3.4"
	},
	"devDependencies": {
		"@tarojs/mini-runner": "2.0.0",
		"@tarojs/plugin-babel": "2.0.0",
		"@tarojs/plugin-csso": "2.0.0",
		"@tarojs/plugin-less": "2.0.0",
		"@tarojs/plugin-uglifyjs": "2.0.0",
		"@tarojs/webpack-runner": "2.0.0",
		"@types/react": "16.3.14",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/parser": "^2.0.0",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"eslint": "^6.3.0",
		"eslint-config-taro": "2.0.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^1.6.1",
		"eslint-plugin-taro": "2.0.0",
		"stylelint": "^13.0.0",
		"stylelint-config-prettier": "^8.0.1",
		"stylelint-prettier": "^1.1.2",
		"typescript": "^3.7.2"
	}
}
